{
  "name": "nq-connector",
  "version": "0.0.0",
  "author": "NQ Framework",
  "description": "A client application for managing nq framework and connecting to the on premise database",
  "main": "dist/electron/main",
  "license": "MIT",
  "files": [
    "dist",
    "*.js"
  ],
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "npm run build:ui && npm run build:electron",
    "build:ui": "ng build --base-href ./",
    "build:electron": "tsc -p ./electron/tsconfig.json",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "npm run lint:ui && npm run lint:electron",
    "lint:ui": "ng lint",
    "lint:electron": "tslint -p ./electron/tsconfig.json -c ./tslint.json",
    "e2e": "ng e2e"
  },
  "private": true,
  "config": {
    "forge": {
      "electronPackagerConfig": {
        "asar": "true"
      },
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "nq-connector"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@teamhive/lottie-player": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.34",
    "codelyzer": "^5.1.2",
    "electron": "^8.2.0",
    "electron-packager": "^14.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "puppeteer": "^2.1.1",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.1",
    "typescript": "~3.8.3"
  }
}
